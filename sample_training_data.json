{
  "data_sources": {
    "aws_cost_explorer": {
      "description": "Real AWS cost data from Cost Explorer API",
      "data_points": 365,
      "date_range": "2024-01-01 to 2024-12-31",
      "services": ["EC2", "RDS", "S3", "Lambda", "CloudFront", "ALB", "EBS"],
      "regions": ["us-east-1", "us-west-2", "eu-west-1", "ap-southeast-1"],
      "metrics": ["BlendedCost", "UnblendedCost", "UsageQuantity"],
      "sample_data": [
        {
          "date": "2024-01-01",
          "service": "EC2",
          "region": "us-east-1",
          "cost": 125.50,
          "usage": 24.5,
          "instance_type": "t3.medium"
        },
        {
          "date": "2024-01-01",
          "service": "RDS",
          "region": "us-east-1",
          "cost": 89.25,
          "usage": 12.3,
          "instance_type": "db.t3.micro"
        }
      ]
    },
    "aws_cur": {
      "description": "AWS Cost and Usage Reports for detailed billing",
      "granularity": "hourly",
      "line_items": 50000,
      "tags": ["Environment", "Project", "Owner", "CostCenter"],
      "sample_line_item": {
        "lineItemId": "1234567890",
        "timePeriod": "2024-01-01T00:00:00Z/2024-01-01T01:00:00Z",
        "productCode": "AmazonEC2",
        "usageType": "BoxUsage:t3.medium",
        "operation": "RunInstances",
        "availabilityZone": "us-east-1a",
        "resourceId": "i-1234567890abcdef0",
        "cost": 0.0525,
        "currency": "USD"
      }
    },
    "cloudwatch_metrics": {
      "description": "Resource utilization metrics from CloudWatch",
      "metrics": [
        "CPUUtilization",
        "NetworkIn",
        "NetworkOut",
        "DiskReadOps",
        "DiskWriteOps",
        "MemoryUtilization"
      ],
      "sample_data": {
        "timestamp": "2024-01-01T00:00:00Z",
        "instanceId": "i-1234567890abcdef0",
        "cpuUtilization": 45.2,
        "memoryUtilization": 67.8,
        "networkIn": 1024.5,
        "networkOut": 2048.3
      }
    },
    "synthetic_data": {
      "description": "Synthetic data for ML model training and demonstration",
      "purpose": "Demonstrate ML capabilities when real data is limited",
      "generated_features": [
        "cost_trends",
        "anomaly_patterns",
        "seasonal_patterns",
        "optimization_scenarios"
      ],
      "sample_synthetic": {
        "date": "2024-01-01",
        "base_cost": 1000,
        "trend_factor": 0.05,
        "seasonal_factor": 0.1,
        "noise_factor": 0.02,
        "predicted_cost": 1050.0,
        "anomaly_score": 0.15
      }
    }
  },
  "ml_features": {
    "cost_features": [
      "daily_cost",
      "weekly_avg_cost",
      "monthly_avg_cost",
      "cost_trend",
      "cost_volatility",
