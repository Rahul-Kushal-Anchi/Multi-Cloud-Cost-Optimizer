# AWS Cost Optimizer Web Application Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_WS_URL=ws://localhost:8080/ws

# AWS Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_AWS_ACCOUNT_ID=123456789012

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ALERTS=true
REACT_APP_ENABLE_OPTIMIZATION=true
REACT_APP_ENABLE_REAL_TIME=true

# External Services
REACT_APP_SLACK_WEBHOOK_URL=
REACT_APP_TEAMS_WEBHOOK_URL=
REACT_APP_JIRA_URL=
REACT_APP_PAGERDUTY_API_KEY=
REACT_APP_DATADOG_API_KEY=

# Development
REACT_APP_DEBUG=true
REACT_APP_MOCK_DATA=true
